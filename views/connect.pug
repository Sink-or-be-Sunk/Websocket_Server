extends layout


block content
	.page-header
		h3 Connect Device
		hr
	form.form-horizontal(method='POST')
		input(type='hidden', name='_csrf', value=_csrf)
		.form-group.row.justify-content-md-center
			label.col-sm-2.col-form-label.text-right.font-weight-bold(for='name') Search
			.col-sm-8
				input.form-control(type='text', name='search', id='search', autofocus=true)
	div(id="devices")
		.card(id="temp") temp
		.card(id="tom") tom

	script(src="/js/connect.js")
