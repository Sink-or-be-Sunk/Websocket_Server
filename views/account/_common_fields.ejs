<dl class="form-group">
	<dt><label for="displayName">Display Name</label></dt>
	<dd>
		<input
			class="form-control"
			type="text"
			value="<%- user.profile.name %>"
			name="displayName"
			placeholder="Enter Display Name"
			required
		/>
	</dd>
</dl>
<dl class="form-group">
	<dt><label for="email">Email</label></dt>
	<dd>
		<input
			class="form-control"
			type="text"
			value="<%- user.email %>"
			type="email"
			name="email"
			placeholder="Enter Email"
			required
		/>
	</dd>
</dl>
