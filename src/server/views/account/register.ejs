<h2>Register</h2>
<form action="/account/register" method="POST">
	<%- include('_common_fields.ejs') %>
	<dl class="form-group">
		<dt><label for="password">Password</label></dt>
		<dd>
			<input
				class="form-control"
				type="password"
				name="password"
				placeholder="Enter Password"
				required
			/>
		</dd>
	</dl>
	<button type="submit" class="btn-primary btn">Create Account</button>
</form>
